{
  "name": "puppet-github_actions_runner",
  "version": "1.0.0",
  "author": "Vox Pupuli",
  "summary": "Module to configure our GitHub Actions runner on Debian hosts",
  "license": "Apache-2.0",
  "source": "https://github.com/voxpupuli/puppet-github-actions-runner",
  "project_page": "https://github.com/voxpupuli/puppet-github-actions-runner",
  "issues_url": "https://github.com/voxpupuli/puppet-github-actions-runner/issues",
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 8.4.0 < 10.0.0"
    },
    {
      "name": "puppet/systemd",
      "version_requirement": ">= 7.1.0 < 8.0.0"
    },
    {
      "name": "puppet-archive",
      "version_requirement": ">= 1.1.2 < 8.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "11",
        "12"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "9"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "24.04"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 7.0.0 < 9.0.0"
    }
  ]
}
